<?xml version="1.0" encoding="UTF-8"?><xmlgui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="\..\xsd\xmlInterface.xsd">	<widgets>		<!-- widget - window komponente allgemein uiType - WidgetStyle -->		<widget id="Fenster" type="window" uiType="">			<!-- properties beschreiben widget -->			<properties>				<property name="width">100</property>				<property name="height">100</property>				<property name="xCoord">0</property>				<property name="yCoord">0</property>				<property name="title">FensterTitel</property>			</properties>			<widgets>				<widget id="MenuBar" type="menuBar">					<widgets>						<widget id="FileMenu" type="Menu">							<properties>								<property name="text">File</property>								<!-- menueintrag file reagiert auf Alt+F -->								<property name="mnemonic">Alt+F</property>							</properties>							<widgets>								<widget id="open" type="MenuItem">									<properties>										<property name="text">Oeffnen...</property>									</properties>								</widget>								<widget id="ende" type="MenuItem">									<properties>										<property name="text">Exit...</property>									</properties>								</widget>							</widgets>						</widget>					</widgets>				</widget>				<widget id="Knopf" type="button" uiType="">					<properties>						<property name="width">10</property>						<property name="height">10</property>						<property name="xCoord">0</property>						<property name="yCoord">0</property>						<property name="text">ButtonText</property>					</properties>				</widget>				<widget id="TextField1" type="textfield">					<properties>						<property name="width">10</property>						<property name="height">10</property>						<property name="xCoord">0</property>						<property name="yCoord">0</property>						<property name="text">Initialisierungstext des Textfeldes</property>					</properties>				</widget>				<widget id="ComboBox1" type="combobox">					<properties>						<property name="width">10</property>						<property name="height">10</property>						<property name="xCoord">0</property>						<property name="yCoord">0</property>						<property name="text">ButtonText</property>					</properties>					<widgets>						<widget id="element1" type="comboboxElement">							<properties>								<property name="text">Element1</property>							</properties>						</widget>						<widget id="element2" type="button">							<properties>								<property name="text">Element2</property>							</properties>						</widget>					</widgets>				</widget>				<widget id="CheckBox1" type="checkox">					<properties>						<property name="width">10</property>						<property name="height">10</property>						<property name="xCoord">0</property>						<property name="yCoord">0</property>						<property name="text">ChecBoxText</property>						<!-- checked vom type boolean -->						<property name="checked">false</property>					</properties>				</widget>			</widgets>		</widget>	</widgets></xmlgui>