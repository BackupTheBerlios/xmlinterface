<?xml version="1.0" encoding="ISO-8859-1"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.0">	<xsd:element name="xmlgui">		<xsd:annotation>			<xsd:documentation>XML Interface for platform indepent GUI description</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:sequence>				<xsd:element name="widgets" maxOccurs="unbounded">					<xsd:complexType>						<xsd:complexContent>							<xsd:extension base="widgets"/>						</xsd:complexContent>					</xsd:complexType>				</xsd:element>			</xsd:sequence>			<xsd:attribute name="magic" type="__string" use="optional"/>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="widgets">		<xsd:sequence>			<xsd:element name="widget" maxOccurs="unbounded">				<xsd:complexType>					<xsd:complexContent>						<xsd:extension base="widget"/>					</xsd:complexContent>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="widget">		<xsd:sequence>			<xsd:element name="properties" minOccurs="0">				<xsd:complexType>					<xsd:complexContent>						<xsd:extension base="properties"/>					</xsd:complexContent>				</xsd:complexType>			</xsd:element>			<xsd:element name="widgets" minOccurs="0">				<xsd:complexType>					<xsd:complexContent>						<xsd:extension base="widgets"/>					</xsd:complexContent>				</xsd:complexType>			</xsd:element>		</xsd:sequence>		<xsd:attribute name="id" type="xsd:string" use="required"/>		<xsd:attribute name="type" type="xsd:string" use="required"/>		<xsd:attribute name="uiType" type="xsd:string" use="optional"/>	</xsd:complexType>	<xsd:complexType name="properties">		<xsd:sequence>			<xsd:element name="property" maxOccurs="unbounded">				<xsd:complexType>					<xsd:simpleContent>						<xsd:extension base="property"/>					</xsd:simpleContent>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="property">		<xsd:simpleContent>			<xsd:extension base="__string">				<xsd:attribute name="name" type="xsd:string" use="required"/>			</xsd:extension>		</xsd:simpleContent>	</xsd:complexType>	<xsd:simpleType name="__string">		<xsd:restriction base="xsd:string">			<xsd:minLength value="1"/>			<xsd:maxLength value="255"/>			<xsd:whiteSpace value="preserve"/>		</xsd:restriction>	</xsd:simpleType></xsd:schema>